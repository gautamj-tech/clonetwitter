{
  "name": "twitter-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "build:css": "tailwind build src/index.css -o dist/style.css",
    "backend": "json-server --watch db.json --port 5000"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.4",
    "core-js": "^3.6.5",
